package model;

public class Course {

    private final String code;

    private final String name;

    private final String credits;

    private final String module;

    public Course(String code, String name, String credits, String module) {
        this.code = code;
        this.name = name;
        this.credits = credits;
        this.module = module;
    }

    public String getCode() {
        return code;
    }

    public String getName() {
        return name;
    }

    public String getCredits() {
        return credits;
    }

    public String getModule() {
        return module;
    }

    @Override
    public String toString() {
        return "Course{" +
                "code='" + code + '\'' +
                ", name='" + name + '\'' +
                ", credits='" + credits + '\'' +
                ", module='" + module + '\'' +
                '}';
    }
}
